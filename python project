        "### Ans 6"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 26,
      "id": "437c69b5",
      "metadata": {
        "id": "437c69b5",
        "colab": {
          "base_uri": "https://localhost:8080/"
